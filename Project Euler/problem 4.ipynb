   "source": [
    "def isPalindrome(n):\n",
    "    a = str(n)\n",
    "    if a != a[::-1]:\n",
    "        return False\n",
    "    return True\n",
    "\n",
    "greatest = 0\n",
    "for i in range(100,1000):\n",
    "    for j in range(100,1000):\n",
    "        if isPalindrome(i*j) == True and i*j > greatest:\n",
    "            greatest = i*j\n",
    "print(greatest)"
   ]
